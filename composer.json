{
    "name": "webup/helium-core",
    "description": "Laravel Admin made with Helium UI",
    "keywords": [
        "webup",
        "laravel",
        "helium-core"
    ],
    "homepage": "https://github.com/webup/helium-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Agence Webup",
            "email": "contact@agence-webup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^11.8",
        "illuminate/support": "^11.8",
        "laravel/prompts": "^0.1",
        "livewire/livewire": "^3.5"
    },
    "require-dev": {
        "orchestra/testbench": "^9.1",
        "pestphp/pest": "^2.34"
    },
    "autoload": {
        "psr-4": {
            "Webup\\Helium\\": "src/",
            "Webup\\Helium\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webup\\Helium\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webup\\Helium\\HeliumServiceProvider"
            ],
            "aliases": {
                "Helium": "Webup\\Helium\\Facades\\Helium"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
